<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <%- include('./partials/nav.ejs') %>
    <main class="main-section">
      <h1>Contacts:</h1>
      <ul>
        <% if (contacts.length) { %> <% contacts.forEach(({ link, name }) => {
        %>
        <li>
          <a href="<%= link %>"><%= name %></a>
        </li>
        <% }) %> <% } %>
      </ul>
    </main>
  </body>
</html>
